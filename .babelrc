{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 7.8
        },
        "loose": true
      }
    ]
  ],
  "plugins": [
    "transform-export-extensions",
    "transform-decorators-legacy",
    "transform-class-properties",
    "babel-plugin-add-module-exports"
  ],
  "minified": true
}
